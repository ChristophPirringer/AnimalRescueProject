<div class="panel panel-defaul">
  <div class="panel-heading"><h2>Animals in the care of <i><%= @organization.name() %></i>:</h2></div>
  <div class="panel-body">

<%if @organization.parent_tickets.any? %>

    <ul class="fa-ul">
        <% @organization.parent_tickets.each do |parent_ticket| %>
        <li><a href="/organizations/<%= @organization.id() %>/parent_tickets/<%= parent_ticket.id %>"> <i class="fa fa-paw"></i></i><span class="list-left"><%= parent_ticket.name() %></a></li>
        <% end %>
    </ul>

<% else %>
        <p>There are no Animals in the care of <i><%= @organization.name() %></i> yet!</p>
<% end %>
</div>
</div>
